{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Folders": [
    {
      "Path": "[Full Folder Path]",
      "MaximumFileAgeInDays": 7,
      "UseRecycleBin": true,
      "Recursive": true
    }
  ],
  "Service": {
    "IntervalInMilliseconds": 3600000
  }
}